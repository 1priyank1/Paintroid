<?xml version="1.0" encoding="utf-8"?>

<!-- This is the Layout for the Overwrite Dialog-->

<RelativeLayout 
xmlns:android="http://schemas.android.com/apk/res/android"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content">
 
 
 
 <TextView android:id="@+id/overwrite_tview_Text"
 android:layout_width="wrap_content" 
 android:layout_height="wrap_content"
 />
 
   
  <Button android:id="@+id/overwrite_btn_Yes" 
    android:layout_below="@+id/overwrite_tview_Text"
 	android:layout_width="wrap_content" 
 	android:layout_height="wrap_content"
 	android:layout_centerHorizontal="true" 
 	android:paddingLeft="10dip" 
  />
  
 <Button android:id="@+id/overwrite_btn_Cancel" 
    android:layout_below="@+id/overwrite_tview_Text"
	android:layout_toRightOf="@id/overwrite_btn_Yes"
 	android:layout_width="wrap_content" 
 	android:layout_height="wrap_content"
 	android:paddingLeft="5dip"
 	android:paddingRight="10dip"
  />
 
</RelativeLayout>